# Example environment variables for local development.
# Copy to .env and fill with your own values. Do NOT commit .env.

# Binance
BINANCE_API_KEY=
BINANCE_SECRET=

# AWS / S3 (CryptoHFTData)
AWS_REGION=ap-northeast-1
S3_BUCKET=amzn-tdata
S3_PREFIX=hftdata

# Optional: in-memory safety cap for parquet auto-sort (rows).
# 32GB RAM recommendation: 20000000
BTENGINE_SORT_ROW_LIMIT=20000000

# Optional: only needed if you are not using the default AWS credential chain
# (IAM role, shared credentials file, SSO, etc.)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=
